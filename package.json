{
  "name": "@d0whc3r/tradingview-bot",
  "version": "0.0.1",
  "private": true,
  "description": "Leonardo monorepo",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:d0whc3r/tradingview-bot.git"
  },
  "author": "d0whc3r",
  "license": "ISC",
  "main": "dist/server.cjs.js",
  "module": "dist/server.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "lint:stage": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "start": "dotenv -e .env -- node --require ts-node/register src/server.ts",
    "startts": "ts-node src/server.ts",
    "startnode": "nodemon",
    "rollup:build": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
    "rollup:watch": "rollup -c -w --environment INCLUDE_DEPS,BUILD:development",
    "build:full": "run-p types rollup:build",
    "watch": "run-p types rollup:watch",
    "build": "cross-env NODE_ENV=production run-s clean:build build:full",
    "types": "dts-bundle-generator --config dts-bundle-generator.js",
    "docs": "rimraf docs && typedoc --theme markdown --namedAnchors",
    "test": "echo run it manualy; exit 0; dotenv -e .env.test -- jest",
    "test:coverage": "echo run it manualy; exit 0; yarn test --coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "yarn lint --fix",
    "clean:build": "rimraf dist",
    "clean:install": "rimraf node_modules",
    "clean": "run-p clean:build clean:install",
    "ci": "run-s clean && rm -fr node_modules && yarn"
  },
  "dependencies": {
    "binance-api-node": "0.11.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-winston": "4.1.0",
    "logdna-winston": "3.0.5",
    "node-binance-api": "0.12.5",
    "node-telegram-bot-api": "0.53.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.14.6",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.4",
    "@types/node-telegram-bot-api": "0.51.1",
    "@types/rollup-plugin-node-builtins": "2.1.1",
    "@types/rollup-plugin-node-globals": "1.4.0",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.5",
    "builtin-modules": "3.2.0",
    "commitizen": "4.2.4",
    "core-js": "3.15.1",
    "cross-env": "7.0.3",
    "dotenv-cli": "4.0.0",
    "dts-bundle-generator": "5.9.0",
    "esbuild": "0.12.9",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "husky": "6.0.0",
    "jest": "27.0.5",
    "jest-chain": "1.1.5",
    "jest-extended": "0.11.5",
    "jest-junit": "12.2.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "11.0.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "rollup": "2.52.3",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-esbuild": "4.5.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typedoc": "0.21.1",
    "typescript": "4.3.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:stage"
    ]
  }
}
